    "eslint": "^7.23.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-import": "^2.22.1",
    "mocha": "^8.3.2",
    "sinon-chai": "^3.6.0",
    "ts-mocha": "^8.0.0",
    "typescript": "^4.2.3"
